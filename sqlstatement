CREATE TABLE resa (
resa_id INT(6) AUTO_INCREMENT PRIMARY KEY,
user_id INT(11),
creationdate DATE,
dates VARCHAR(40),
datestart DATE,
dateend DATE,
people INT(3),
active BOOL,
confirmed BOOL,
closed BOOL
);
CREATE TABLE user (
user_id INT(11) AUTO_INCREMENT PRIMARY KEY,
username VARCHAR(20),
password VARCHAR(100),
email VARCHAR(50),
telephone VARCHAR(50),
settings VARCHAR(32500),
tracking VARCHAR(32500),
rank INT(3),
firstName VARCHAR(100),
lastName VARCHAR(100)
);

CREATE TABLE resauni (
resauni_id INT(11) AUTO_INCREMENT PRIMARY KEY,
resa_id INT (11),
user_id INT(11),
name VARCHAR(50),
datestart VARCHAR(100),
dateend VARCHAR(50),
days INT(6),
mbPack BOOL,
alpBoots BOOL,
alpBootSize VARCHAR(50),
crampons BOOL,
iceAxe BOOL,
helmet BOOL,
harness BOOL,
gaiters BOOL,
poles BOOL,
techIceAxes BOOL,
backpack BOOL,
climbingShoes BOOL,
climbingShoesSize VARCHAR(50),
kidClimbingShoes BOOL,
kidClimbingShoesSize VARCHAR(50),
gaillandKit BOOL,
crashpad BOOL,
hikingShoes BOOL,
hikingShoesSize VARCHAR(50),
babyCarrier BOOL,
stroller BOOL,
viaFerrataShock BOOL,
viaFerrataKit BOOL,
slackline BOOL
);

